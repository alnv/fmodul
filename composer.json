{
  "name": "fmodule/fmodule",
  "description": "Mit dem F Modul kannst du eigene Backend Module erstellen. Erzeuge schnell und einfach Übersichtsseiten, Detailseiten, Filter Formularen und Taxonomies. Mache dein Backend aufgeräumter und benutzerfreundlicher.",
  "keywords": [
    "filter",
    "custom",
    "catalog",
    "meta",
    "tags",
    "taxonomy",
    "contao",
    "google-maps",
    "frontend-editing"
  ],
  "type": "contao-module",
  "homepage": "http://fmodul.alexandernaumov.de",
  "license": "commercial",
  "authors": [
    {
      "name": "Alexander Naumov",
      "homepage": "http://www.alexandernaumov.de",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "me@alexandernaumov.de",
    "issues": "https://github.com/alnv/fmodul/issues",
    "wiki": "http://fmodul.alexandernaumov.de/ressourcen.html"
  },
  "require": {
    "php": ">=5.4",
    "contao/core-bundle":"~3.5 || ~4.1"
  },
  "autoload": {
    "classmap": [
      "src/"
    ]
  },
  "replace": {
    "contao-legacy/f-modul": "*"
  },
  "extra": {
    "contao": {
      "sources": {
        "": "system/modules/fmodule"
      }
    }
  }
}