{
  "name": "alnv/fmodule",
  "description": "Create your own backend modules for Contao CMS",
  "keywords": [ "contao", "filter", "catalog", "tags", "taxonomy", "catalog" ],
  "type": "contao-module",
  "homepage": "https://fmodul.alexandernaumov.de",
  "license": "commercial",
  "authors": [
    {
      "name": "Alexander Naumov",
      "homepage": "https://www.alexandernaumov.de",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "me@alexandernaumov.de",
    "issues": "https://github.com/alnv/fmodul/issues",
    "wiki": "https://fmodul.alexandernaumov.de/ressourcen.html"
  },
  "require": {
    "php": ">=5.4",
    "contao/core-bundle":"~3.5 || ~4.1",
    "contao-community-alliance/composer-plugin":"^2.4.1 || ~3.0"
  },
  "autoload": {
    "classmap": [
      "src/"
    ]
  },
  "replace": {
    "contao-legacy/fmodule": "self.version"
  },
  "extra": {
    "contao": {
      "sources": {
        "": "system/modules/fmodule"
      }
    }
  }
}